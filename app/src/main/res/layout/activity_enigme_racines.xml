        android:onClick="onClick"
        android:text="ToggleButton"
        android:textOff="OFF"
        android:textOn="ON"
        android:background="@drawable/border"
        android:textColor="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
